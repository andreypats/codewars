function correct(string)
{
  let arr = string.split('')
	for (let i = 0; i < arr.length; i++) {
    if (arr[i] === '0') arr[i] = 'O'
    if (arr[i] === '5') arr[i] = 'S'
    if (arr[i] === '1') arr[i] = 'I'    
  }   
  return arr.join('');
}


function correct(string) {
    return string.replace(/0/g, "O")
                 .replace(/5/g, "S")
                 .replace(/1/g, "I");
}
