function squareDigits(num){
  let str = num.toString();
  let newStr = '';
  for (let i = 0; i < str.length; i++) {
    newStr += (Number(str[i])*Number(str[i])).toString();
  }
  return Number(newStr);
}


function squareDigits(num){
  return Number(('' + num).split('').map(function (val) { return val * val;}).join(''));
  
}
